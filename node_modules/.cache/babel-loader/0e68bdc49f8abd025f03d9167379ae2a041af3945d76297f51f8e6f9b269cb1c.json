der\">\n        <h1>Your Bookmarks</h1>\n        <div class=\"search-container\">\n          <input \n            v-model=\"searchQuery\" \n            type=\"text\" \n            placeholder=\"Search Bookmarks...\" \n          />\n        </div>\n      </header>\n\n      <!-- Create Bookmark Section -->\n      <div class=\"create-bookmark\">\n        <CreateBookmark @create-bookmark=\"addBookmark\" />\n      </div>\n\n      <!-- Bookmark List -->\n      <div class=\"bookmark-list-container\">\n        <div class=\"bookmark-list\">\n          <BookmarkList \n            :bookmarks=\"filteredBookmarks\" \n            @delete-bookmark=\"deleteBookmark\" \n            @update-bookmark=\"updateBookmark\" \n          />\n        </div>\n      </div>\n\n      <AppFooter />\n    </div>\n  </div>\n</template>\n\n<script>\nimport { ref, computed, onMounted, watch } from \"vue\";\nimport AppFooter from \"./components/AppFooter.vue\";\nimport CreateBookmark from \"./components/CreateBookmark.vue\";\nimport BookmarkList from \"./components/BookmarkList.vue\";\n\nexport default {\n  components: {\n    AppFooter,\n    CreateBookmark,\n    BookmarkList,\n  },\n  setup() {\n    const bookmarks = ref([]);\n    const searchQuery = ref(\"\");\n    const darkMode = ref(false);\n\n    // Toggle dark mode\n    const toggleDarkMode = () => {\n      darkMode.value = !darkMode.value;\n      localStorage.setItem(\"darkMode\", darkMode.value);\n    };\n\n    // Load from localStorage when the app starts\n    onMounted(() => {\n      const savedBookmarks = localStorage.getItem(\"bookmarks\");\n      if (savedBookmarks) {\n        bookmarks.value = JSON.parse(savedBookmarks);\n      }\n\n      // Load dark mode preference from localStorage\n      const savedDarkMode = localStorage.getItem(\"darkMode\") === \"true\";\n      darkMode.value = savedDarkMode;\n    });\n\n    // Computed property for searched bookmarks\n    const filteredBookmarks = computed(() => {\n      return bookmarks.value.filter((bookmark) => {\n        return (\n          bookmark.title.toLowerCase().includes(searchQuery.value.toLowerCase()) ||\n          bookmark.tags.some((tag) =>\n            tag.toLowerCase().includes(searchQuery.value.toLowerCase())\n          )\n        );\n      });\n    });\n\n    // Add Bookmark\n    const addBookmark = (bookmark) => {\n      bookmarks.value.push({\n        id: Date.now(), // Unique ID\n        ...bookmark,\n        tags: bookmark.tags.split(\",\").map((tag) => tag.trim()),\n      });\n    };\n\n    // Delete Bookmark\n    const deleteBookmark = (id) => {\n      bookmarks.value = bookmarks.value.filter(\n        (bookmark) => bookmark.id !== id\n      );\n    };\n\n    // Update Bookmark\n    const updateBookmark = (updatedBookmark) => {\n      const index = bookmarks.value.findIndex(\n        (bookmark) => bookmark.id === updatedBookmark.id\n      );\n      if (index !== -1) {\n        bookmarks.value[index] = { ...updatedBookmark };\n      }\n    };\n\n    // Save bookmarks to localStorage\n    watch(bookmarks, (newBookmarks) => {\n      localStorage.setItem(\"bookmarks\", JSON.stringify(newBookmarks));\n    }, { deep: true });\n\n    return {\n      bookmarks,\n      addBookmark,\n      deleteBookmark,\n      updateBookmark,\n      searchQuery,\n      filteredBookmarks,\n      darkMode,\n      toggleDarkMode,\n    };\n  },\n};\n</script>\n\n<style scoped>\n.app-container {\n  display: flex;\n  min-height: 100vh;\n  font-family: Arial, sans-serif;\n}\n\n.light {\n  background-color: #f0f0f0;\n  color: #333;\n}\n\n.dark {\n  background-color: #181818;\n  color: #f0f0f0;\n}\n\n.sidebar {\n  width: 250px;\n  background-color: #0070f3;\n  padding: 20px;\n  color: white;\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  justify-content: space-between;\n}\n\n.app-title {\n  font-size: 24px;\n  font-weight: bold;\n  text-align: center;\n}\n\n.toggle-btn {\n  margin-top: 20px;\n  background-color: transparent;\n  color: white;\n  border: 1px solid white;\n  padding: 10px;\n  cursor: pointer;\n  font-size: 14px;\n  border-radius: 5px;\n}\n\n.toggle-btn:hover {\n  background-color: rgba(255, 255, 255, 0.1);\n}\n\n.main-content {\n  flex-grow: 1;\n  padding: 40px;\n}\n\n.header {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n}\n\n.search-container input {\n  padding: 8px 16px;\n  font-size: 14px;\n  border-radius: 5px;\n  border: 1px solid #ddd;\n}\n\n.create-bookmark {\n  margin-top: 30px;\n}\n\n.bookmark-list-container {\n  margin-top: 30px;\n  display: flex;\n  flex-wrap: wrap;\n  gap: 20px;\n}\n\n.bookmark-list {\n  display: grid;\n  grid-template-columns: repeat(auto-fill, minmax(250px, 1fr));\n  gap: 20px;\n}\n\n.bookmark-card {\n  background-color: white;\n  border-radius: 8px;\n  padding: 20px;\n  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\n  transition: transform 0.3s;\n}\n\n.bookmark-card:hover {\n  transform: scale(1.05);\n}\n\n.bookmark-card h3 {\n  margin: 0;\n  font-size: 18px;\n}\n\n.bookmark-card p {\n  margin: 10px 0;\n  font-size: 14px;\n}\n\n.bookmark-card button {\n  padding: 8px 16px;\n  background-color: #0070f3;\n  color: white;\n  border: none;\n  border-radius: 5px;\n  cursor: pointer;\n}\n\n.bookmark-card button:hover {\n  background-color: #0056c1;\n}\n\n.app-footer {\n  background-color: #0070f3;\n  color: white;\n  padding: 10px;\n  text-align: center;\n}\n</style>\n"],"mappings":";;EAGSA,KAAK,EAAC;AAAS;;EAQfA,KAAK,EAAC;AAAc;;EACfA,KAAK,EAAC;AAAQ;;EAEfA,KAAK,EAAC;AAAkB;;EAU1BA,KAAK,EAAC;AAAiB;;EAKvBA,KAAK,EAAC;AAAyB;;EAC7BA,KAAK,EAAC;AAAe;;;;;uBA7BhCC,mBAAA,CAwCM;IAxCAD,KAAK,EADbE,eAAA,mBACiCC,MAAA,CAAAC,QAAQ;MACrCC,mBAAA,4BAA+B,EAC/BC,mBAAA,CAKM,OALNC,UAKM,G,0BAJJD,mBAAA,CAA2C;IAAvCN,KAAK,EAAC;EAAW,GAAC,kBAAgB,sBACtCM,mBAAA,CAES;IAFDN,KAAK,EAAC,YAAY;IAAEQ,OAAK,EAAAC,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAEP,MAAA,CAAAQ,cAAA,IAAAR,MAAA,CAAAQ,cAAA,IAAAD,IAAA,CAAc;sBAC5CP,MAAA,CAAAC,QAAQ,8C,GAIfC,mBAAA,kBAAqB,EACrBC,mBAAA,CA6BM,OA7BNM,UA6BM,GA5B