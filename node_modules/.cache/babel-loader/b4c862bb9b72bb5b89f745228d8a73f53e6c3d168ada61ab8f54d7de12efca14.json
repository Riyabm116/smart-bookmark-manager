{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref } from 'vue';\nimport Header from './components/Header.vue';\nimport Footer from './components/Footer.vue';\nimport BookmarkForm from './components/BookmarkForm.vue';\nimport BookmarkList from './components/BookmarkList.vue';\nexport default {\n  components: {\n    Header,\n    Footer,\n    BookmarkForm,\n    BookmarkList\n  },\n  setup() {\n    const darkMode = ref(false);\n    const bookmarks = ref(JSON.parse(localStorage.getItem('bookmarks')) || []);\n    const toggleDarkMode = () => {\n      darkMode.value = !darkMode.value;\n    };\n    const addBookmark = bookmark => {\n      bookmarks.value.push(bookmark);\n      localStorage.setItem('bookmarks', JSON.stringify(bookmarks.value));\n    };\n    const editBookmark = (index, updatedBookmark) => {\n      bookmarks.value[index] = updatedBookmark;\n      localStorage.setItem('bookmarks', JSON.stringify(bookmarks.value));\n    };\n    const deleteBookmark = index => {\n      bookmarks.value.splice(index, 1);\n      localStorage.setItem('bookmarks', JSON.stringify(bookmarks.value));\n    };\n    return {\n      darkMode,\n      bookmarks,\n      toggleDarkMode,\n      addBookmark,\n      editBookmark,\n      deleteBookmark\n    };\n  }\n};","map":{"version":3,"names":["ref","Header","Footer","BookmarkForm","BookmarkList","components","setup","darkMode","bookmarks","JSON","parse","localStorage","getItem","toggleDarkMode","value","addBookmark","bookmark","push","setItem","stringify","editBookmark","index","updatedBookmark","deleteBookmark","splice"],"sources":["/Users/riyabm/Desktop/smart-bookmark-manager/src/App.vue"],"sourcesContent":["<template>\n  <div :class=\"{'dark-mode': darkMode}\">\n    <Header @toggle-dark-mode=\"toggleDarkMode\" />\n    <div class=\"main-content\">\n      <BookmarkForm @add-bookmark=\"addBookmark\" />\n      <BookmarkList \n        :bookmarks=\"bookmarks\" \n        @edit-bookmark=\"editBookmark\" \n        @delete-bookmark=\"deleteBookmark\"\n      />\n    </div>\n    <Footer />\n  </div>\n</template>\n\n<script>\nimport { ref } from 'vue';\nimport Header from './components/Header.vue';\nimport Footer from './components/Footer.vue';\nimport BookmarkForm from './components/BookmarkForm.vue';\nimport BookmarkList from './components/BookmarkList.vue';\n\nexport default {\n  components: {\n    Header,\n    Footer,\n    BookmarkForm,\n    BookmarkList,\n  },\n  setup() {\n    const darkMode = ref(false);\n    const bookmarks = ref(JSON.parse(localStorage.getItem('bookmarks')) || []);\n\n    const toggleDarkMode = () => {\n      darkMode.value = !darkMode.value;\n    };\n\n    const addBookmark = (bookmark) => {\n      bookmarks.value.push(bookmark);\n      localStorage.setItem('bookmarks', JSON.stringify(bookmarks.value));\n    };\n\n    const editBookmark = (index, updatedBookmark) => {\n      bookmarks.value[index] = updatedBookmark;\n      localStorage.setItem('bookmarks', JSON.stringify(bookmarks.value));\n    };\n\n    const deleteBookmark = (index) => {\n      bookmarks.value.splice(index, 1);\n      localStorage.setItem('bookmarks', JSON.stringify(bookmarks.value));\n    };\n\n    return { darkMode, bookmarks, toggleDarkMode, addBookmark, editBookmark, deleteBookmark };\n  }\n};\n</script>\n"],"mappings":";AAgBA,SAASA,GAAE,QAAS,KAAK;AACzB,OAAOC,MAAK,MAAO,yBAAyB;AAC5C,OAAOC,MAAK,MAAO,yBAAyB;AAC5C,OAAOC,YAAW,MAAO,+BAA+B;AACxD,OAAOC,YAAW,MAAO,+BAA+B;AAExD,eAAe;EACbC,UAAU,EAAE;IACVJ,MAAM;IACNC,MAAM;IACNC,YAAY;IACZC;EACF,CAAC;EACDE,KAAKA,CAAA,EAAG;IACN,MAAMC,QAAO,GAAIP,GAAG,CAAC,KAAK,CAAC;IAC3B,MAAMQ,SAAQ,GAAIR,GAAG,CAACS,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,WAAW,CAAC,KAAK,EAAE,CAAC;IAE1E,MAAMC,cAAa,GAAIA,CAAA,KAAM;MAC3BN,QAAQ,CAACO,KAAI,GAAI,CAACP,QAAQ,CAACO,KAAK;IAClC,CAAC;IAED,MAAMC,WAAU,GAAKC,QAAQ,IAAK;MAChCR,SAAS,CAACM,KAAK,CAACG,IAAI,CAACD,QAAQ,CAAC;MAC9BL,YAAY,CAACO,OAAO,CAAC,WAAW,EAAET,IAAI,CAACU,SAAS,CAACX,SAAS,CAACM,KAAK,CAAC,CAAC;IACpE,CAAC;IAED,MAAMM,YAAW,GAA