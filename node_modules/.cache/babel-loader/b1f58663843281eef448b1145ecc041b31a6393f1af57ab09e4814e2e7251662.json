kmarkList.vue\";\n\nexport default {\n  components: {\n    AppHeader,\n    AppFooter,\n    CreateBookmark, // Using the new design\n    BookmarkList,\n  },\n  setup() {\n    const bookmarks = ref([]);\n\n    const addBookmark = (bookmark) => {\n      bookmarks.value.push({\n        ...bookmark,\n        tags: bookmark.tags.split(\",\").map(tag => tag.trim()),\n      });\n    };\n\n    const deleteBookmark = (index) => {\n      bookmarks.value.splice(index, 1);\n    };\n\n    return { bookmarks, addBookmark, deleteBookmark };\n  }\n};\n</script>\n\n<style>\n* {\n  box-sizing: border-box;\n}\n\nhtml, body {\n  height: 100%;\n  margin: 0;\n  font-family: Arial, sans-serif;\n}\n\n.app-container {\n  display: flex;\n  flex-direction: column;\n  height: 100vh;\n}\n\n.main-container {\n  flex: 1; /* Ensures main content expands to available space */\n  padding: 20px;\n  overflow-y: auto; /* Enables scrolling if content overflows */\n  background-color: #f5f5f5;\n}\n\n/* Footer always at the bottom */\nfooter {\n  margin-top: auto;\n}\n</style>\n"],"mappings":";;;AAeA,SAASA,GAAE,QAAS,KAAK;AACzB,OAAOC,SAAQ,MAAO,4BAA4B;AAClD,OAAOC,SAAQ,MAAO,4BAA4B;AAClD,OAAOC,cAAa,MAAO,iCAAiC,EAAE;AAC9D,OAAOC,YAAW,MAAO,+BAA+B;AAExD,eAAe;EACbC,UAAU,EAAE;IACVJ,SAAS;IACTC,SAAS;IACTC,cAAc;IAAE;IAChBC;EACF,CAAC;EACDE,KAAKA,CAAA,EAAG;IACN,MAAMC,SAAQ,GAAIP,GAAG,CAAC,EAAE,CAAC;IAEzB,MAAMQ,WAAU,GAAKC,QAAQ,IAAK;MAChCF,SAAS,CAACG,KAAK,CAACC,IAAI,CAAC;QACnB,GAAGF,QAAQ;QACXG,IAAI,EAAEH,QAAQ,CAACG,IAAI,CAACC,KAAK,CAAC,GAAG,CAAC,CAACC,GAAG,CAACC,GAAE,IAAKA,GAAG,CAACC,IAAI,CAAC,CAAC;MACtD,CAAC,CAAC;IACJ,CAAC;IAED,MAAMC,cAAa,GAAKC,KAAK,IAAK;MAChCX,SAAS,CAACG,KAAK,CAACS,MAAM,CAACD,KAAK,EAAE,CAAC,CAAC;IAClC,CAAC;IAED,OAAO;MAAEX,SAAS;MAAEC,WAAW;MAAES;IAAe,CAAC;EACnD;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}