\";\nimport CreateBookmark from \"./components/CreateBookmark.vue\";\nimport BookmarkList from \"./components/BookmarkList.vue\";\n\nexport default {\n  components: { AppHeader, AppFooter, CreateBookmark, BookmarkList },\n  setup() {\n    const bookmarks = ref([]);\n    const searchQuery = ref(\"\");\n    const darkMode = ref(false);\n    const categories = ref([\"All\", \"Work\", \"Personal\", \"Learning\", \"Entertainment\"]);\n    const selectedCategory = ref(\"All\");\n\n    const filteredBookmarks = computed(() => {\n      return bookmarks.value.filter(bookmark => {\n        const matchesSearch = bookmark.title.toLowerCase().includes(searchQuery.value.toLowerCase());\n        const matchesCategory = selectedCategory.value === \"All\" || bookmark.category === selectedCategory.value;\n        return matchesSearch && matchesCategory;\n      });\n    });\n\n    const addBookmark = (bookmark) => {\n      bookmarks.value.push({ id: Date.now(), ...bookmark });\n    };\n\n    const deleteBookmark = (id) => {\n      bookmarks.value = bookmarks.value.filter(bookmark => bookmark.id !== id);\n    };\n\n    const updateBookmark = (updatedBookmark) => {\n      const index = bookmarks.value.findIndex(bookmark => bookmark.id === updatedBookmark.id);\n      if (index !== -1) bookmarks.value[index] = { ...updatedBookmark };\n    };\n\n    const filterByCategory = (category) => {\n      selectedCategory.value = category;\n    };\n\n    const toggleDarkMode = () => {\n      darkMode.value = !darkMode.value;\n      document.body.classList.toggle(\"dark-mode\", darkMode.value);\n    };\n\n    return { bookmarks, searchQuery, addBookmark, deleteBookmark, updateBookmark, filteredBookmarks, categories, filterByCategory, toggleDarkMode };\n  }\n};\n</script>\n\n<style scoped>\n.dashboard {\n  display: flex;\n  flex-direction: column;\n  height: 100vh;\n}\n\n.dashboard-container {\n  display: flex;\n  flex: 1;\n}\n\n.sidebar {\n  width: 200px;\n  background: #f4f4f4;\n  padding: 1rem;\n}