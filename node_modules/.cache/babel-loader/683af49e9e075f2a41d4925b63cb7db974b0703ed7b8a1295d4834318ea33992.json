{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nexport default {\n  data() {\n    return {\n      bookmark: {\n        title: '',\n        url: '',\n        tags: '',\n        category: ''\n      },\n      bookmarks: [],\n      isDarkMode: false\n    };\n  },\n  methods: {\n    addBookmark() {\n      this.$emit('create-bookmark', this.bookmark);\n      this.bookmarks.push({\n        id: Date.now(),\n        ...this.bookmark,\n        tags: this.bookmark.tags.split(',').map(tag => tag.trim())\n      });\n      this.clearForm();\n    },\n    saveBookmark() {\n      this.saveToLocalStorage();\n      this.clearForm();\n    },\n    saveToLocalStorage() {\n      localStorage.setItem('bookmarks', JSON.stringify(this.bookmarks));\n    },\n    deleteBookmark(id) {\n      this.bookmarks = this.bookmarks.filter(bookmark => bookmark.id !== id);\n      this.saveToLocalStorage();\n    },\n    toggleDarkMode() {\n      this.isDarkMode = !this.isDarkMode;\n    },\n    clearForm() {\n      this.bookmark = {\n        title: '',\n        url: '',\n        tags: '',\n        category: ''\n      };\n    }\n  },\n  mounted() {\n    const savedBookmarks = JSON.parse(localStorage.getItem('bookmarks')) || [];\n    this.bookmarks = savedBookmarks;\n  }\n};","map":{"version":3,"names":["data","bookmark","title","url","tags","category","bookmarks","isDarkMode","methods","addBookmark","$emit","push","id","Date","now","split","map","tag","trim","clearForm","saveBookmark","saveToLocalStorage","localStorage","setItem","JSON","stringify","deleteBookmark","filter","toggleDarkMode","mounted","savedBookmarks","parse","getItem"],"sources":["/Users/riyabm/Desktop/smart-bookmark-manager/src/App.vue"],"sourcesContent":["<template>\n  <div :class=\"['app-container', { dark: isDarkMode }]\">\n    <div class=\"dashboard\">\n      <!-- Header Section -->\n      <div class=\"header\">\n        <h1>Bookmark Manager</h1>\n        <button @click=\"toggleDarkMode\" class=\"toggle-btn\">\n          Toggle Mode\n        </button>\n      </div>\n\n      <!-- Create Bookmark Section -->\n      <div class=\"card\">\n        <h2>Create a Bookmark</h2>\n        <form @submit.prevent=\"submitBookmark\">\n          <div class=\"form-group\">\n            <label for=\"title\">Title</label>\n            <input v-model=\"bookmark.title\" type=\"text\" id=\"title\" placeholder=\"Enter title\" />\n          </div>\n          <div class=\"form-group\">\n            <label for=\"url\">URL</label>\n            <input v-model=\"bookmark.url\" type=\"url\" id=\"url\" placeholder=\"Enter URL\" />\n          </div>\n          <div class=\"form-group\">\n            <label for=\"tags\">Tags</label>\n            <input v-model=\"bookmark.tags\" type=\"text\" id=\"tags\" placeholder=\"Comma-separated tags\" />\n          </div>\n          <div class=\"form-group\">\n            <label for=\"category\">Category</label>\n            <select v-model=\"bookmark.category\" id=\"category\">\n              <option value=\"\">Select Category</option>\n              <option value=\"Work\">Work</option>\n              <option value=\"Personal\">Personal</option>\n              <option value=\"Other\">Other</option>\n            </select>\n          </div>\n          <div class=\"button-group\">\n            <button type=\"submit\" @click.prevent=\"addBookmark\" class=\"btn-add\">Add Bookmark</button>\n            <button @click.prevent=\"saveBookmark\" class=\"btn-save\">Save Bookmark</button>\n          </div>\n        </form>\n      </div>\n\n      <!-- Bookmark List Section -->\n      <div class=\"card\">\n        <h2>Your Bookmarks</h2>\n        <div class=\"bookmark-list\">\n          <div v-for=\"bookmark in bookmarks\" :key=\"bookmark.id\" class=\"bookmark-card\">\n            <div class=\"bookmark-header\">\n              <h3>{{ bookmark.title }}</h3>\n              <button @click=\"deleteBookmark(bookmark.id)\" class=\"btn-delete\">Delete</button>\n            </div>\n            <p><strong>URL:</strong> <a :href=\"bookmark.url\" target=\"_blank\">{{ bookmark.url }}</a></p>\n            <p><strong>Tags:</strong> {{ bookmark.tags.join(', ') }}</p>\n            <p><strong>Category:</strong> {{ bookmark.category }}</p>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      bookmark: {\n        title: '',\n        url: '',\n        tags: '',\n        category: '',\n      },\n      bookmarks: [],\n      isDarkMode: false,\n    };\n  },\n  methods: {\n    addBookmark() {\n      this.$emit('create-bookmark', this.bookmark);\n      this.bookmarks.push({\n        id: Date.now(),\n        ...this.bookmark,\n        tags: this.bookmark.tags.split(',').map(tag => tag.trim()),\n      });\n      this.clearForm();\n    },\n\n    saveBookmark() {\n      this.saveToLocalStorage();\n      this.clearForm();\n    },\n\n    saveToLocalStorage() {\n      localStorage.setItem('bookmarks', JSON.stringify(this.bookmarks));\n    },\n\n    deleteBookmark(id) {\n      this.bookmarks = this.bookmarks.filter(bookmark => bookmark.id !== id);\n      this.saveToLocalStorage();\n    },\n\n    toggleDarkMode() {\n      this.isDarkMode = !this.isDarkMode;\n    },\n\n    clearForm() {\n      this.bookmark = { title: '', url: '', tags: '', category: '' };\n    },\n  },\n\n  mounted() {\n    const savedBookmarks = JSON.parse(localStorage.getItem('bookmarks')) || [];\n    this.bookmarks = savedBookmarks;\n  },\n};\n</script>\n\n<style scoped>\n/* Basic Reset */\n* {\n  margin: 0;\n  padding: 0;\n  box-sizing: border-box;\n}\n\nbody {\n  font-family: 'Arial', sans-serif;\n}\n\n.app-container {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  flex-direction: column;\n  padding: 20px;\n  background-color: #f4f4f4;\n  transition: background-color 0.3s;\n}\n\n.dark .app-container {\n  background-color: #333;\n  color: white;\n}\n\n.dashboard {\n  width: 100%;\n  max-width: 1200px;\n}\n\n.header {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  margin-bottom: 20px;\n}\n\n.header h1 {\n  font-size: 2rem;\n  color: inherit;\n}\n\n.toggle-btn {\n  padding: 10px 15px;\n  cursor: pointer;\n  background-color: #0070f3;\n  color: white;\n  border: none;\n  border-radius: 5px;\n}\n\n.toggle-btn:hover {\n  background-color: #0056c1;\n}\n\n/* Card styling */\n.card {\n  background-color: white;\n  border-radius: 8px;\n  padding: 20px;\n  margin-bottom: 20px;\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\n}\n\n.card h2 {\n  font-size: 1.5rem;\n  margin-bottom: 15px;\n}\n\n.form-group {\n  margin-bottom: 15px;\n}\n\n.form-group label {\n  display: block;\n  font-weight: bold;\n  margin-bottom: 5px;\n  font-size: 14px;\n}\n\ninput[type=\"text\"],\ninput[type=\"url\"],\nselect {\n  width: 100%;\n  padding: 10px;\n  border-radius: 5px;\n  border: 1px solid #ddd;\n  font-size: 14px;\n  margin-top: 5px;\n}\n\nbutton {\n  padding: 10px 20px;\n  font-size: 14px;\n  border-radius: 5px;\n  border: none;\n  cursor: pointer;\n}\n\nbutton:hover {\n  opacity: 0.8;\n}\n\n.btn-add {\n  background-color: #0070f3;\n  color: white;\n}\n\n.btn-save {\n  background-color: #28a745;\n  color: white;\n}\n\n.btn-add:hover {\n  background-color: #0056c1;\n}\n\n.btn-save:hover {\n  background-color: #218838;\n}\n\n.bookmark-list {\n  display: grid;\n  grid-template-columns: 1fr 1fr;\n  gap: 20px;\n}\n\n.bookmark-card {\n  background-color: #f9f9f9;\n  padding: 15px;\n  border-radius: 8px;\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\n}\n\n.bookmark-header {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n}\n\n.bookmark-header h3 {\n  font-size: 1.1rem;\n}\n\n.btn-delete {\n  background-color: red;\n  color: white;\n  border: none;\n  border-radius: 5px;\n  cursor: pointer;\n}\n\n.btn-delete:hover {\n  background-color: darkred;\n}\n\n/* Dark Mode Styling */\n.dark .card {\n  background-color: #444;\n  color: white;\n}\n\n.dark .form-group label {\n  color: white;\n}\n\n.dark input[type=\"text\"],\n.dark input[type=\"url\"],\n.dark select {\n  background-color: #555;\n  color: white;\n  border: 1px solid #666;\n}\n\n.dark .btn-add,\n.dark .btn-save {\n  background-color: #0056c1;\n}\n\n.dark .btn-add:hover,\n.dark .btn-save:hover {\n  background-color: #003d82;\n}\n</style>\n"],"mappings":";;;;AA+DA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE;QACRC,KAAK,EAAE,EAAE;QACTC,GAAG,EAAE,EAAE;QACPC,IAAI,EAAE,EAAE;QACRC,QAAQ,EAAE;MACZ,CAAC;MACDC,SAAS,EAAE,EAAE;MACbC,UAAU,EAAE;IACd,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,WAAWA,CAAA,EAAG;MACZ,IAAI,CAACC,KAAK,CAAC,iBAAiB,EAAE,IAAI,CAACT,QAAQ,CAAC;MAC5C,IAAI,CAACK,SAAS,CAACK,IAAI,CAAC;QAClBC,EAAE,EAAEC,IAAI,CAACC,GAAG,CAAC,CAAC;QACd,GAAG,IAAI,CAACb,QAAQ;QAChBG,IAAI,EAAE,IAAI,CAACH,QAAQ,CAACG,IAAI,CAACW,KAAK,CAAC,GAAG,CAAC,CAACC,GAAG,CAACC,GAAE,IAAKA,GAAG,CAACC,IAAI,CAAC,CAAC;MAC3D,CAAC,CAAC;MACF,IAAI,CAACC,SAAS,CAAC,CAAC;IAClB,CAAC;IAEDC,YAAYA,CAAA,EAAG;MACb,IAAI,CAACC,kBAAkB,CAAC,CAAC;MACzB,IAAI,CAACF,SAAS,CAAC,CAAC;IAClB,CAAC;IAEDE,kBAAkBA,CAAA,EAAG;MACnBC,YAAY,CAACC,OAAO,CAAC,WAAW,EAAEC,IAAI,CAACC,SAAS,CAAC,IAAI,CAACnB,SAAS,CAAC,CAAC;IACnE,CAAC;IAEDoB,cAAcA,CAACd,EAAE,EAAE;MACjB,IAAI,CAACN,SAAQ,GAAI,IAAI,CAACA,SAAS,CAACqB,MAAM,CAAC1B,QAAO,IAAKA,QAAQ,CAACW,EAAC,KAAMA,EAAE,CAAC;MACtE,IAAI,CAACS,kBAAkB,CAAC,CAAC;IAC3B,CAAC;IAEDO,cAAcA,CAAA,EAAG;MACf,IAAI,CAACrB,UAAS,GAAI,CAAC,IAAI,CAACA,UAAU;IACpC,CAAC;IAEDY,SAASA,CAAA,EAAG;MACV,IAAI,CAAClB,QAAO,GAAI;QAAEC,KAAK,EAAE,EAAE;QAAEC,GAAG,EAAE,EAAE;QAAEC,IAAI,EAAE,EAAE;QAAEC,QAAQ,EAAE;MAAG,CAAC;IAChE;EACF,CAAC;EAEDwB,OAAOA,CAAA,EAAG;IACR,MAAMC,cAAa,GAAIN,IAAI,CAACO,KAAK,CAACT,YAAY,CAACU,OAAO,CAAC,WAAW,CAAC,KAAK,EAAE;IAC1E,IAAI,CAAC1B,SAAQ,GAAIwB,cAAc;EACjC;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependenc