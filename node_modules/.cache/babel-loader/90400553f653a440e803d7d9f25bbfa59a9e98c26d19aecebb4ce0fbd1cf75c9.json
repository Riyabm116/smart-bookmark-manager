{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport \"core-js/modules/es.iterator.some.js\";\nimport { ref, computed, onMounted, watch } from \"vue\";\nimport AppFooter from \"./components/AppFooter.vue\";\nimport CreateBookmark from \"./components/CreateBookmark.vue\";\nimport BookmarkList from \"./components/BookmarkList.vue\";\nexport default {\n  components: {\n    AppFooter,\n    CreateBookmark,\n    BookmarkList\n  },\n  setup() {\n    const bookmarks = ref([]);\n    const searchQuery = ref(\"\");\n    const darkMode = ref(false);\n    const isModalOpen = ref(false);\n\n    // Toggle dark mode\n    const toggleDarkMode = () => {\n      darkMode.value = !darkMode.value;\n      localStorage.setItem(\"darkMode\", darkMode.value);\n    };\n\n    // Load bookmarks and dark mode preference from localStorage\n    onMounted(() => {\n      const savedBookmarks = localStorage.getItem(\"bookmarks\");\n      if (savedBookmarks) {\n        bookmarks.value = JSON.parse(savedBookmarks);\n      }\n      const savedDarkMode = localStorage.getItem(\"darkMode\") === \"true\";\n      darkMode.value = savedDarkMode;\n    });\n\n    // Computed property for filtered bookmarks based on search query\n    const filteredBookmarks = computed(() => {\n      return bookmarks.value.filter(bookmark => {\n        return bookmark.title.toLowerCase().includes(searchQuery.value.toLowerCase()) || bookmark.tags.some(tag => tag.toLowerCase().includes(searchQuery.value.toLowerCase()));\n      });\n    });\n\n    // Add bookmark to list\n    const addBookmark = bookmark => {\n      bookmarks.value.push({\n        id: Date.now(),\n        ...bookmark,\n        tags: bookmark.tags.split(\",\").map(tag => tag.trim())\n      });\n      closeModal();\n    };\n\n    // Delete bookmark\n    const deleteBookmark = id => {\n      bookmarks.value = bookmarks.value.filter(bookmark => bookmark.id !== id);\n    };\n\n    // Update bookmark\n    const updateBookmark = updatedBookmark => {\n      const index = bookmarks.value.findIndex(bookmark => bookmark.id === updatedBookmark.id);\n      if (index !== -1) {\n        bookmarks.value[index] = {\n          ...updatedBookmark\n        };\n      }\n    };\n\n    // Save bookmarks to localStorage whenever it changes\n    watch(bookmarks, newBookmarks => {\n      localStorage.setItem(\"bookmarks\", JSON.stringify(newBookmarks));\n    }, {\n      deep: true\n    });\n\n    // Open and close the bookmark creation modal\n    const openBookmarkForm = () => {\n      isModalOpen.value = true;\n    };\n    const closeModal = () => {\n      isModalOpen.value = false;\n    };\n    return {\n      bookmarks,\n      searchQuery,\n      darkMode,\n      toggleDarkMode,\n      filteredBookmarks,\n      addBookmark,\n      deleteBookmark,\n      updateBookmark,\n      isModalOpen,\n      openBookmarkForm,\n      closeModal\n    };\n  }\n};","map":{"version":3,"names":["ref","computed","onMounted","watch","AppFooter","CreateBookmark","BookmarkList","components","setup","bookmarks","searchQuery","darkMode","isModalOpen","toggleDarkMode","value","localStorage","setItem","savedBookmarks","getItem","JSON","parse","savedDarkMode","filteredBookmarks","filter","bookmark","title","toLowerCase","includes","tags","some","tag","addBookmark","push","id","Date","now","split","map","trim","closeModal","deleteBookmark","updateBookmark","updatedBookmark","index","findIndex","newBookmarks","stringify","deep","openBookmarkForm"],"sources":["/Users/riyabm/Desktop/smart-bookmark-manager/src/App.vue"],"sourcesContent":["<template>\n  <div :class=\"['app-container', darkMode ? 'dark' : 'light']\">\n    <!-- Sidebar Section -->\n    <div class=\"sidebar\">\n      <h2 class=\"app-title\">Smart Bookmark Manager</h2>\n      <div class=\"toggle-btn-container\">\n        <button class=\"toggle-btn\" @click=\"toggleDarkMode\">\n          {{ darkMode ? 'Switch to Light Mode' : 'Switch to Dark Mode' }}\n        </button>\n      </div>\n    </div>\n\n    <!-- Main Content Section -->\n    <div class=\"main-content\">\n      <!-- Header -->\n      <div class=\"header\">\n        <div class=\"search-container\">\n          <input\n            v-model=\"searchQuery\"\n            type=\"text\"\n            class=\"search-input\"\n            placeholder=\"Search Bookmarks...\"\n          />\n        </div>\n        <button class=\"add-bookmark-btn\" @click=\"openBookmarkForm\">+ Add Bookmark</button>\n      </div>\n\n      <!-- Bookmark List Section -->\n      <div class=\"bookmark-list-container\">\n        <BookmarkList\n          :bookmarks=\"filteredBookmarks\"\n          @delete-bookmark=\"deleteBookmark\"\n          @update-bookmark=\"updateBookmark\"\n        />\n      </div>\n\n      <!-- Footer -->\n      <AppFooter />\n    </div>\n\n    <!-- Create Bookmark Modal -->\n    <CreateBookmark\n      v-if=\"isModalOpen\"\n      @create-bookmark=\"addBookmark\"\n      @close-modal=\"closeModal\"\n    />\n  </div>\n</template>\n\n<script>\nimport { ref, computed, onMounted, watch } from \"vue\";\nimport AppFooter from \"./components/AppFooter.vue\";\nimport CreateBookmark from \"./components/CreateBookmark.vue\";\nimport BookmarkList from \"./components/BookmarkList.vue\";\n\nexport default {\n  components: {\n    AppFooter,\n    CreateBookmark,\n    BookmarkList,\n  },\n  setup() {\n    const bookmarks = ref([]);\n    const searchQuery = ref(\"\");\n    const darkMode = ref(false);\n    const isModalOpen = ref(false);\n\n    // Toggle dark mode\n    const toggleDarkMode = () => {\n      darkMode.value = !darkMode.value;\n      localStorage.setItem(\"darkMode\", darkMode.value);\n    };\n\n    // Load bookmarks and dark mode preference from localStorage\n    onMounted(() => {\n      const savedBookmarks = localStorage.getItem(\"bookmarks\");\n      if (savedBookmarks) {\n        bookmarks.value = JSON.parse(savedBookmarks);\n      }\n\n      const savedDarkMode = localStorage.getItem(\"darkMode\") === \"true\";\n      darkMode.value = savedDarkMode;\n    });\n\n    // Computed property for filtered bookmarks based on search query\n    const filteredBookmarks = computed(() => {\n      return bookmarks.value.filter((bookmark) => {\n        return (\n          bookmark.title.toLowerCase().includes(searchQuery.value.toLowerCase()) ||\n          bookmark.tags.some((tag) =>\n            tag.toLowerCase().includes(searchQuery.value.toLowerCase())\n          )\n        );\n      });\n    });\n\n    // Add bookmark to list\n    const addBookmark = (bookmark) => {\n      bookmarks.value.push({\n        id: Date.now(),\n        ...bookmark,\n        tags: bookmark.tags.split(\",\").map((tag) => tag.trim()),\n      });\n      closeModal();\n    };\n\n    // Delete bookmark\n    const deleteBookmark = (id) => {\n      bookmarks.value = bookmarks.value.filter(\n        (bookmark) => bookmark.id !== id\n      );\n    };\n\n    // Update bookmark\n    const updateBookmark = (updatedBookmark) => {\n      const index = bookmarks.value.findIndex(\n        (bookmark) => bookmark.id === updatedBookmark.id\n      );\n      if (index !== -1) {\n        bookmarks.value[index] = { ...updatedBookmark };\n      }\n    };\n\n    // Save bookmarks to localStorage whenever it changes\n    watch(bookmarks, (newBookmarks) => {\n      localStorage.setItem(\"bookmarks\", JSON.stringify(newBookmarks));\n    }, { deep: true });\n\n    // Open and close the bookmark creation modal\n    const openBookmarkForm = () => {\n      isModalOpen.value = true;\n    };\n    const closeModal = () => {\n      isModalOpen.value = false;\n    };\n\n    return {\n      bookmarks,\n      searchQuery,\n      darkMode,\n      toggleDarkMode,\n      filteredBookmarks,\n      addBookmark,\n      deleteBookmark,\n      updateBookmark,\n      isModalOpen,\n      openBookmarkForm,\n      closeModal,\n    };\n  },\n};\n</script>\n\n<style scoped>\n.app-container {\n  display: flex;\n  font-family: 'Roboto', sans-serif;\n  min-height: 100vh;\n}\n\n.light {\n  background-color: #f4f7fc;\n  color: #333;\n}\n\n.dark {\n  background-color: #181818;\n  color: #e8e8e8;\n}\n\n.sidebar {\n  width: 300px;\n  background-color: #0070f3;\n  padding: 30px 20px;\n  color: white;\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n}\n\n.app-title {\n  font-size: 26px;\n  font-weight: bold;\n  margin-bottom: 20px;\n}\n\n.toggle-btn-container {\n  margin-top: auto;\n}\n\n.toggle-btn {\n  padding: 10px 20px;\n  font-size: 14px;\n  background-color: transparent;\n  color: white;\n  border: 1px solid white;\n  border-radius: 5px;\n  cursor: pointer;\n}\n\n.toggle-btn:hover {\n  background-color: rgba(255, 255, 255, 0.1);\n}\n\n.main-content {\n  flex-grow: 1;\n  padding: 40px;\n}\n\n.header {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  margin-bottom: 30px;\n}\n\n.search-container {\n  flex-grow: 1;\n}\n\n.search-input {\n  width: 100%;\n  padding: 10px 15px;\n  font-size: 16px;\n  border: 1px solid #ddd;\n  border-radius: 5px;\n}\n\n.add-bookmark-btn {\n  padding: 12px 20px;\n  font-size: 16px;\n  background-color: #0070f3;\n  color: white;\n  border: none;\n  border-radius: 5px;\n  cursor: pointer;\n}\n\n.add-bookmark-btn:hover {\n  background-color: #0056c1;\n}\n\n.bookmark-list-container {\n  display: flex;\n  flex-wrap: wrap;\n  gap: 20px;\n}\n\n.bookmark-list {\n  display: grid;\n  grid-template-columns: repeat(auto-fill, minmax(250px, 1fr));\n  gap: 20px;\n}\n\n.bookmark-card {\n  background-color: white;\n  border-radius: 8px;\n  padding: 20px;\n  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\n  transition: transform 0.3s;\n}\n\n.bookmark-card:hover {\n  transform: scale(1.05);\n}\n\n.bookmark-card h3 {\n  margin: 0;\n  font-size: 18px;\n}\n\n.bookmark-card p {\n  margin: 10px 0;\n  font-size: 14px;\n}\n\n.bookmark-card button {\n  padding: 8px 16px;\n  background-color: #0070f3;\n  color: white;\n  border: none;\n  border-radius: 5px;\n  cursor: pointer;\n}\n\n.bookmark-card button:hover {\n  background-color: #0056c1;\n}\n\n.app-footer {\n  background-color: #0070f3;\n  color: white;\n  padding: 20px;\n  text-align: center;\n}\n</style>\n"],"mappings":";;;;;AAkDA,SAASA,GAAG,EAAEC,QAAQ,EAAEC,SAAS,EAAEC,KAAI,QAAS,KAAK;AACrD,OAAOC,SAAQ,MAAO,4BAA4B;AAClD,OAAOC,cAAa,MAAO,iCAAiC;AAC5D,OAAOC,YAAW,MAAO,+BAA+B;AAExD,eAAe;EACbC,UAAU,EAAE;IACVH,SAAS;IACTC,cAAc;IACdC;EACF,CAAC;EACDE,KAAKA,CAAA,EAAG;IACN,MAAMC,SAAQ,GAAIT,GAAG,CAAC,EAAE,CAAC;IACzB,MAAMU,WAAU,GAAIV,GAAG,CAAC,EAAE,CAAC;IAC3B,MAAMW,QAAO,GAAIX,GAAG,CAAC,KAAK,CAAC;IAC3B,MAAMY,WAAU,GAAIZ,GAAG,CAAC,KAAK,CAAC;;IAE9B;IACA,MAAMa,cAAa,GAAIA,CAAA,KAAM;MAC3BF,QAAQ,CAACG,KAAI,GAAI,CAACH,QAAQ,CAACG,KAAK;MAChCC,YAAY,CAACC,OAAO,CAAC,UAAU,EAAEL,QAAQ,CAACG,KAAK,CAAC;IAClD,CAAC;;IAED;IACAZ,SAAS,CAAC,MAAM;MACd,MAAMe,cAAa,GAAIF,YAAY,CAACG,OAAO,CAAC,WAAW,CAAC;MACxD,IAAID,cAAc,EAAE;QAClBR,SAAS,CAACK,KAAI,GAAIK,IAAI,CAACC,KAAK,CAACH,cAAc,CAAC;MAC9C;MAEA,MAAMI,aAAY,GAAIN,YAAY,CAACG,OAAO,CAAC,UAAU,MAAM,MAAM;MACjEP,QAAQ,CAACG,KAAI,GAAIO,aAAa;IAChC,CAAC,CAAC;;IAEF;IACA,MAAMC,iBAAgB,GAAIrB,QAAQ,CAAC,MAAM;MACvC,OAAOQ,SAAS,CAACK,KAAK,CAACS,MAAM,CAAEC,QAAQ,IAAK;QAC1C,OACEA,QAAQ,CAACC,KAAK,CAACC,WAAW,CAAC,CAAC,CAACC,QAAQ,CAACjB,WAAW,CAACI,KAAK,CAACY,WAAW,CAAC,CAAC,KACrEF,QAAQ,CAACI,IAAI,CAACC,IAAI,CAAEC,GAAG,IACrBA,GAAG,CAACJ,WAAW,CAAC,CAAC,CAACC,QAAQ,CAACjB,WAAW,CAACI,KAAK,CAACY,WAAW,CAAC,CAAC,CAC5D;MAEJ,CAAC,CAAC;IACJ,CAAC,CAAC;;IAEF;IACA,MAAMK,WAAU,GAAKP,QAAQ,IAAK;MAChCf,SAAS,CAACK,KAAK,CAACkB,IAAI,CAAC;QACnBC,EAAE,EAAEC,IAAI,CAACC,GAAG,CAAC,CAAC;QACd,GAAGX,QAAQ;QACXI,IAAI,EAAEJ,QAAQ,CAACI,IAAI,CAACQ,KAAK,CAAC,GAAG,CAAC,CAACC,GAAG,CAAEP,GAAG,IAAKA,GAAG,CAACQ,IAAI,CAAC,CAAC;MACxD,CAAC,CAAC;MACFC,UAAU,CAAC,CAAC;IACd,CAAC;;IAED;IACA,MAAMC,cAAa,GAAKP,EAAE,IAAK;MAC7BxB,SAAS,CAACK,KAAI,GAAIL,SAAS,CAACK,KAAK,CAACS,MAAM,CACrCC,QAAQ,IAAKA,QAAQ,CAACS,EAAC,K