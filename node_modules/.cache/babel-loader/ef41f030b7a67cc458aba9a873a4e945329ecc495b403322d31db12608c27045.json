', darkMode ? 'dark' : 'light']\">\n    <!-- Sidebar Section -->\n    <div class=\"sidebar\">\n      <h2 class=\"app-title\">Smart Bookmark Manager</h2>\n      <div class=\"toggle-btn-container\">\n        <button class=\"toggle-btn\" @click=\"toggleDarkMode\">\n          {{ darkMode ? 'Switch to Light Mode' : 'Switch to Dark Mode' }}\n        </button>\n      </div>\n    </div>\n\n    <!-- Main Content Section -->\n    <div class=\"main-content\">\n      <!-- Header -->\n      <div class=\"header\">\n        <div class=\"search-container\">\n          <input\n            v-model=\"searchQuery\"\n            type=\"text\"\n            class=\"search-input\"\n            placeholder=\"Search Bookmarks...\"\n          />\n        </div>\n        <button class=\"add-bookmark-btn\" @click=\"openBookmarkForm\">+ Add Bookmark</button>\n      </div>\n\n      <!-- Bookmark List Section -->\n      <div class=\"bookmark-list-container\">\n        <BookmarkList\n          :bookmarks=\"filteredBookmarks\"\n          @delete-bookmark=\"deleteBookmark\"\n          @update-bookmark=\"updateBookmark\"\n        />\n      </div>\n\n      <!-- Footer -->\n      <AppFooter />\n    </div>\n\n    <!-- Create Bookmark Modal -->\n    <CreateBookmark\n      v-if=\"isModalOpen\"\n      @create-bookmark=\"addBookmark\"\n      @close-modal=\"closeModal\"\n    />\n  </div>\n</template>\n\n<script>\nimport { ref, computed, onMounted, watch } from \"vue\";\nimport AppFooter from \"./components/AppFooter.vue\";\nimport CreateBookmark from \"./components/CreateBookmark.vue\";\nimport BookmarkList from \"./components/BookmarkList.vue\";\n\nexport default {\n  components: {\n    AppFooter,\n    CreateBookmark,\n    BookmarkList,\n  },\n  setup() {\n    const bookmarks = ref([]);\n    const searchQuery = ref(\"\");\n    const darkMode = ref(false);\n    const isModalOpen = ref(false);\n\n    // Toggle dark mode\n    const toggleDarkMode = () => {\n      darkMode.value = !darkMode.value;\n      localStorage.setItem(\"darkMode\", darkMode.value);\n    };\n\n    // Load bookmarks and dark mode preference from localStorage\n    onMounted(() => {\n      const savedBookmarks = localStorage.getItem(\"bookmarks\");\n      if (savedBookmarks) {\n        bookmarks.value = JSON.parse(savedBookmarks);\n      }\n\n      const savedDarkMode = localStorage.getItem(\"darkMode\") === \"true\";\n      darkMode.value = savedDarkMode;\n    });\n\n    // Computed property for filtered bookmarks based on search query\n    const filteredBookmarks = computed(() => {\n      return bookmarks.value.filter((bookmark) => {\n        return (\n          bookmark.title.toLowerCase().includes(searchQuery.value.toLowerCase()) ||\n          bookmark.tags.some((tag) =>\n            tag.toLowerCase().includes(searchQuery.value.toLowerCase())\n          )\n        );\n      });\n    });\n\n    // Add bookmark to list\n    const addBookmark = (bookmark) => {\n      bookmarks.value.push({\n        id: Date.now(),\n        ...bookmark,\n        tags: bookmark.tags.split(\",\").map((tag) => tag.trim()),\n      });\n      closeModal();\n    };\n\n    // Delete bookmark\n    const deleteBookmark = (id) => {\n      bookmarks.value = bookmarks.value.filter(\n        (bookmark) => bookmark.id !== id\n      );\n    };\n\n    // Update bookmark\n    const updateBookmark = (updatedBookmark) => {\n      const index = bookmarks.value.findIndex(\n        (bookmark) => bookmark.id === updatedBookmark.id\n      );\n      if (index !== -1) {\n        bookmarks.value[index] = { ...updatedBookmark };\n      }\n    };\n\n    // Save bookmarks to localStorage whenever it changes\n    watch(bookmarks, (newBookmarks) => {\n      localStorage.setItem(\"bookmarks\", JSON.stringify(newBookmarks));\n    }, { deep: true });\n\n    // Open and close the bookmark creation modal\n    const openBookmarkForm = () => {\n      isModalOpen.value = true;\n    };\n    const closeModal = () => {\n      isModalOpen.value = false;\n    };\n\n    return {\n      bookmarks,\n      searchQuery,\n      darkMode,\n      toggleDarkMode,\n      filteredBookmarks,\n      addBookmark,\n      deleteBookmark,\n      updateBookmark,\n      isModalOpen,\n      openBookmarkForm,\n      closeModal,\n    };\n  },\n};\n</script>\n\n<style scoped>\n.app-container {\n  display: flex;\n  font-family: 'Roboto', sans-serif;\n  min-height: 100vh;\n}\n\n.light {\n  background-color: #f4f7fc;\n  color: #333;\n}\n\n.dark {\n  background-color: #181818;\n  color: #e8e8e8;\n}\n\n.sidebar {\n  width: 300px;\n  background-color: #0070f3;\n  padding: 30px 20px;\n  color: white;\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n}\n\n.app-title {\n  font-size: 26px;\n  font-weight: bold;\n  margin-bottom: 20px;\n}\n\n.toggle-btn-container {\n  margin-top: auto;\n}\n\n.toggle-btn {\n  padding: 10px 20px;\n  font-size: 14px;\n  background-color: transparent;\n  color: white;\n  border: 1px solid white;\n  border-radius: 5px;\n  cursor: pointer;\n}\n\n.toggle-btn:hover {\n  background-color: rgba(255, 255, 255, 0.1);\n}\n\n.main-content {\n  flex-grow: 1;\n  padding: 40px;\n}\n\n.header {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  margin-bottom: 30px;\n}\n\n.search-container {\n  flex-grow: 1;\n}\n\n.search-input {\n  width: 100%;\n  padding: 10px 15px;\n  font-size: 16px;\n  border: 1px solid #ddd;\n  border-radius: 5px;\n}\n\n.add-bookmark-btn {\n  padding: 12px 20px;\n  font-size: 16px;\n  background-color: #0070f3;\n  color: white;\n  border: none;\n  border-radius: 5px;\n  cursor: pointer;\n}\n\n.add-bookmark-btn:hover {\n  background-color: #0056c1;\n}\n\n.bookmark-list-container {\n  display: flex;\n  flex-wrap: wrap;\n  gap: 20px;\n}\n\n.bookmark-list {\n  display: grid;\n  grid-template-columns: repeat(auto-fill, minmax(250px, 1fr));\n  gap: 20px;\n}\n\n.bookmark-card {\n  background-color: white;\n  border-radius: 8px;\n  padding: 20px;\n  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\n  transition: transform 0.3s;\n}\n\n.bookmark-card:hover {\n  transform: scale(1.05);\n}\n\n.bookmark-card h3 {\n  margin: 0;\n  font-size: 18px;\n}\n\n.bookmark-card p {\n  margin: 10px 0;\n  font-size: 14px;\n}\n\n.bookmark-card button {\n  padding: 8px 16px;\n  background-color: #0070f3;\n  color: white;\n  border: none;\n  border-radius: 5px;\n  cursor: pointer;\n}\n\n.bookmark-card button:hover {\n  background-color: #0056c1;\n}\n\n.app-footer {\n  background-color: #0070f3;\n  color: white;\n  padding: 20px;\n  text-align: center;\n}\n</style>\n"],"mappings":";;EAGSA,KAAK,EAAC;AAAS;;EAEbA,KAAK,EAAC;AAAsB;;EAQ9BA,KAAK,EAAC;AAAc;;EAElBA,KAAK,EAAC;AAAQ;;EACZA,KAAK,EAAC;AAAkB;;EAY1BA,KAAK,EAAC;AAAyB;;;;;uBA3BxCC,mBAAA,CA6CM;IA7CAD,KAAK,EADbE,eAAA,mBACiCC,MAAA,CAAAC,QAAQ;MACrCC,mBAAA,qBAAwB,EACxBC,mBAAA,CAOM,OAPNC,UAOM,G,0BANJD,mBAAA,CAAiD;IAA7CN,KAAK,EAAC;EAAW,GAAC,wBAAsB,sBAC5CM,mBAAA,CAIM,OAJNE,UAIM,GAHJF,mBAAA,CAES;IAFDN,KAAK,EAAC,YAAY;IAAES,OAAK,EAAAC,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAER,MAAA,CAAAS,cAAA,IAAAT,MAAA,CAAAS,cAAA,IAAAD,IAAA,CAAc;sBAC5CR,MAAA,CAAAC,QAAQ,kE,KAKjBC,mBAAA,0BAA6B,EAC7BC,mBAAA,CAyBM,OAzBNO,UAyBM,GAxBJR,mBAAA,YAAe,EACfC,mBAAA,CAUM,OAVNQ,UAUM,GATJR,mBAAA,CAOM,OAPNS,UAOM,G,gBANJT,mBAAA,CAKE;IAtBZ,uBAAAI,MAAA,QAAAA,MAAA,MAAAM,MAAA,IAkBqBb,MAAA,CAAAc,WAAW,GAAAD,MAAA;IACpBE,IAAI,EAAC,MAAM;IACXlB,KAAK,EAAC,cAAc;IACpBmB,WAAW,EAAC;iDAHHhB,MAAA,CAAAc,WAAW,E,KAMxBX,mBAAA,CAAkF;IAA1EN,KAAK,EAAC,kBAAkB;IAAES,OAAK,EAAAC,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAER,MAAA,CAAAiB,gBAAA,IAAAjB,MAAA,CAAAiB,gBAAA,IAAAT,IAAA,CAAgB;KAAE,gBAAc,E,GAG3EN,mBAAA,2BAA8B,EAC9BC,mBAAA,CAMM,OANNe,UAMM,GALJC,YAAA,CAIEC,uBAAA;IAHCC,SAAS,EAAErB,MAAA,CAAAsB,iBAAiB;IAC5BC,gBAAe,EAAEvB,MAAA,CAAAwB,cAAc;IAC/BC,gBAAe,EAAEzB,MAAA,CAAA0B;oFAItBxB,mBAAA,YAAe,EACfiB,YAAA,CAAaQ,oBAAA,E,GAGfzB,mBAAA,2BAA8B,EAEtBF,MAAA,CAAA4B,WAAW,I,cADnBC,YAAA,CAIEC,yBAAA;IA7CNC,GAAA;IA2COC,gBAAe,EAAEhC,MAAA,CAAAiC,WAAW;IAC5BC,YAAW,EAAElC,MAAA,CAAAmC;mEA5CpBjC,mBAAA,e","ignoreList":[]},"metadata":{},"so