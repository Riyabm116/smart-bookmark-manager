{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport \"core-js/modules/es.iterator.some.js\";\nimport { ref, computed, onMounted, watch } from \"vue\";\nimport AppFooter from \"./components/AppFooter.vue\";\nimport CreateBookmark from \"./components/CreateBookmark.vue\";\nimport BookmarkList from \"./components/BookmarkList.vue\";\nexport default {\n  components: {\n    AppFooter,\n    CreateBookmark,\n    BookmarkList\n  },\n  setup() {\n    const bookmarks = ref([]);\n    const savedBookmarks = ref([]);\n    const searchQuery = ref(\"\");\n    const darkMode = ref(false);\n    const isModalOpen = ref(false);\n    const activeSection = ref(\"all\"); // To track which section is active (all or saved)\n\n    // Toggle dark mode\n    const toggleDarkMode = () => {\n      darkMode.value = !darkMode.value;\n      localStorage.setItem(\"darkMode\", darkMode.value);\n    };\n\n    // Load bookmarks and dark mode preference from localStorage\n    onMounted(() => {\n      const savedBookmarksData = localStorage.getItem(\"bookmarks\");\n      if (savedBookmarksData) {\n        bookmarks.value = JSON.parse(savedBookmarksData);\n      }\n      const savedSavedBookmarks = localStorage.getItem(\"savedBookmarks\");\n      if (savedSavedBookmarks) {\n        savedBookmarks.value = JSON.parse(savedSavedBookmarks);\n      }\n      const savedDarkMode = localStorage.getItem(\"darkMode\") === \"true\";\n      darkMode.value = savedDarkMode;\n    });\n\n    // Filter bookmarks based on search query\n    const filteredBookmarks = computed(() => {\n      const bookmarksToFilter = activeSection.value === \"saved\" ? savedBookmarks.value : bookmarks.value;\n      return bookmarksToFilter.filter(bookmark => {\n        return bookmark.title.toLowerCase().includes(searchQuery.value.toLowerCase()) || bookmark.tags.some(tag => tag.toLowerCase().includes(searchQuery.value.toLowerCase()));\n      });\n    });\n\n    // Add bookmark\n    const addBookmark = bookmark => {\n      bookmarks.value.push({\n        id: Date.now(),\n        ...bookmark,\n        tags: bookmark.tags.split(\",\").map(tag => tag.trim())\n      });\n      closeModal();\n    };\n\n    // Save bookmark to saved list\n    const saveBookmark = bookmark => {\n      savedBookmarks.value.push(bookmark);\n      localStorage.setItem(\"savedBookmarks\", JSON.stringify(savedBookmarks.value));\n    };\n\n    // Delete bookmark\n    const deleteBookmark = id => {\n      bookmarks.value = bookmarks.value.filter(bookmark => bookmark.id !== id);\n      savedBookmarks.value = savedBookmarks.value.filter(bookmark => bookmark.id !== id);\n    };\n\n    // Update bookmark\n    const updateBookmark = updatedBookmark => {\n      const index = bookmarks.value.findIndex(bookmark => bookmark.id === updatedBookmark.id);\n      if (index !== -1) {\n        bookmarks.value[index] = {\n          ...updatedBookmark\n        };\n      }\n    };\n\n    // Save bookmarks to localStorage whenever it changes\n    watch(bookmarks, newBookmarks => {\n      localStorage.setItem(\"bookmarks\", JSON.stringify(newBookmarks));\n    }, {\n      deep: true\n    });\n\n    // Switch between All Bookmarks and Saved Bookmarks sections\n    const viewAllBookmarks = () => {\n      activeSection.value = \"all\";\n    };\n    const viewSavedBookmarks = () => {\n      activeSection.value = \"saved\";\n    };\n\n    // Open bookmark creation form\n    const openBookmarkForm = () => {\n      isModalOpen.value = true;\n    };\n\n    // Close modal\n    const closeModal = () => {\n      isModalOpen.value = false;\n    };\n    return {\n      bookmarks,\n      savedBookmarks,\n      searchQuery,\n      darkMode,\n      toggleDarkMode,\n      filteredBookmarks,\n      addBookmark,\n      saveBookmark,\n      deleteBookmark,\n      updateBookmark,\n      isModalOpen,\n      openBookmarkForm,\n      closeModal,\n      activeSection,\n      viewAllBookmarks,\n      viewSavedBookmarks\n    };\n  }\n};","map":{"version":3,"names":["ref","computed","onMounted","watch","AppFooter","CreateBookmark","BookmarkList","components","setup","bookmarks","savedBookmarks","searchQuery","darkMode","isModalOpen","activeSection","toggleDarkMode","value","localStorage","setItem","savedBookmarksData","getItem","JSON","parse","savedSavedBookmarks","savedDarkMode","filteredBookmarks","bookmarksToFilter","filter","bookmark","title","toLowerCase","includes","tags","some","tag","addBookmark","push","id","Date","now","split","map","trim","closeModal","saveBookmark","stringify","deleteBookmark","updateBookmark","updatedBookmark","index","findIndex","newBookmarks","deep","viewAllBookmarks","viewSavedBookmarks","openBookmarkForm"],"sources":["/Users/riyabm/Desktop/smart-bookmark-manager/src/App.vue"],"sourcesContent":["<template>\n  <div :class=\"['app-container', darkMode ? 'dark' : 'light']\">\n    <!-- Sidebar Section -->\n    <div class=\"sidebar\">\n      <h2 class=\"app-title\">Smart Bookmark Manager</h2>\n      <div class=\"nav-links\">\n        <button @click=\"viewAllBookmarks\" class=\"nav-btn\" :class=\"activeSection === 'all' ? 'active' : ''\">All Bookmarks</button>\n        <button @click=\"viewSavedBookmarks\" class=\"nav-btn\" :class=\"activeSection === 'saved' ? 'active' : ''\">Saved Bookmarks</button>\n      </div>\n      <div class=\"toggle-btn-container\">\n        <button class=\"toggle-btn\" @click=\"toggleDarkMode\">\n          {{ darkMode ? 'Switch to Light Mode' : 'Switch to Dark Mode' }}\n        </button>\n      </div>\n    </div>\n\n    <!-- Main Content Section -->\n    <div class=\"main-content\">\n      <!-- Header -->\n      <div class=\"header\">\n        <div class=\"search-container\">\n          <input\n            v-model=\"searchQuery\"\n            type=\"text\"\n            class=\"search-input\"\n            placeholder=\"Search Bookmarks...\"\n          />\n        </div>\n        <button class=\"add-bookmark-btn\" @click=\"openBookmarkForm\">+ Add Bookmark</button>\n      </div>\n\n      <!-- Bookmark List Section -->\n      <div class=\"bookmark-list-container\">\n        <BookmarkList\n          :bookmarks=\"filteredBookmarks\"\n          @delete-bookmark=\"deleteBookmark\"\n          @update-bookmark=\"updateBookmark\"\n        />\n      </div>\n\n      <!-- Footer -->\n      <AppFooter />\n    </div>\n\n    <!-- Create Bookmark Modal -->\n    <CreateBookmark\n      v-if=\"isModalOpen\"\n      @create-bookmark=\"addBookmark\"\n      @close-modal=\"closeModal\"\n    />\n  </div>\n</template>\n\n<script>\nimport { ref, computed, onMounted, watch } from \"vue\";\nimport AppFooter from \"./components/AppFooter.vue\";\nimport CreateBookmark from \"./components/CreateBookmark.vue\";\nimport BookmarkList from \"./components/BookmarkList.vue\";\n\nexport default {\n  components: {\n    AppFooter,\n    CreateBookmark,\n    BookmarkList,\n  },\n  setup() {\n    const bookmarks = ref([]);\n    const savedBookmarks = ref([]);\n    const searchQuery = ref(\"\");\n    const darkMode = ref(false);\n    const isModalOpen = ref(false);\n    const activeSection = ref(\"all\"); // To track which section is active (all or saved)\n\n    // Toggle dark mode\n    const toggleDarkMode = () => {\n      darkMode.value = !darkMode.value;\n      localStorage.setItem(\"darkMode\", darkMode.value);\n    };\n\n    // Load bookmarks and dark mode preference from localStorage\n    onMounted(() => {\n      const savedBookmarksData = localStorage.getItem(\"bookmarks\");\n      if (savedBookmarksData) {\n        bookmarks.value = JSON.parse(savedBookmarksData);\n      }\n\n      const savedSavedBookmarks = localStorage.getItem(\"savedBookmarks\");\n      if (savedSavedBookmarks) {\n        savedBookmarks.value = JSON.parse(savedSavedBookmarks);\n      }\n\n      const savedDarkMode = localStorage.getItem(\"darkMode\") === \"true\";\n      darkMode.value = savedDarkMode;\n    });\n\n    // Filter bookmarks based on search query\n    const filteredBookmarks = computed(() => {\n      const bookmarksToFilter =\n        activeSection.value === \"saved\" ? savedBookmarks.value : bookmarks.value;\n      return bookmarksToFilter.filter((bookmark) => {\n        return (\n          bookmark.title.toLowerCase().includes(searchQuery.value.toLowerCase()) ||\n          bookmark.tags.some((tag) =>\n            tag.toLowerCase().includes(searchQuery.value.toLowerCase())\n          )\n        );\n      });\n    });\n\n    // Add bookmark\n    const addBookmark = (bookmark) => {\n      bookmarks.value.push({\n        id: Date.now(),\n        ...bookmark,\n        tags: bookmark.tags.split(\",\").map((tag) => tag.trim()),\n      });\n      closeModal();\n    };\n\n    // Save bookmark to saved list\n    const saveBookmark = (bookmark) => {\n      savedBookmarks.value.push(bookmark);\n      localStorage.setItem(\"savedBookmarks\", JSON.stringify(savedBookmarks.value));\n    };\n\n    // Delete bookmark\n    const deleteBookmark = (id) => {\n      bookmarks.value = bookmarks.value.filter((bookmark) => bookmark.id !== id);\n      savedBookmarks.value = savedBookmarks.value.filter(\n        (bookmark) => bookmark.id !== id\n      );\n    };\n\n    // Update bookmark\n    const updateBookmark = (updatedBookmark) => {\n      const index = bookmarks.value.findIndex(\n        (bookmark) => bookmark.id === updatedBookmark.id\n      );\n      if (index !== -1) {\n        bookmarks.value[index] = { ...updatedBookmark };\n      }\n    };\n\n    // Save bookmarks to localStorage whenever it changes\n    watch(bookmarks, (newBookmarks) => {\n      localStorage.setItem(\"bookmarks\", JSON.stringify(newBookmarks));\n    }, { deep: true });\n\n    // Switch between All Bookmarks and Saved Bookmarks sections\n    const viewAllBookmarks = () => {\n      activeSection.value = \"all\";\n    };\n    const viewSavedBookmarks = () => {\n      activeSection.value = \"saved\";\n    };\n\n    // Open bookmark creation form\n    const openBookmarkForm = () => {\n      isModalOpen.value = true;\n    };\n\n    // Close modal\n    const closeModal = () => {\n      isModalOpen.value = false;\n    };\n\n    return {\n      bookmarks,\n      savedBookmarks,\n      searchQuery,\n      darkMode,\n      toggleDarkMode,\n      filteredBookmarks,\n      addBookmark,\n      saveBookmark,\n      deleteBookmark,\n      updateBookmark,\n      isModalOpen,\n      openBookmarkForm,\n      closeModal,\n      activeSection,\n      viewAllBookmarks,\n      viewSavedBookmarks,\n    };\n  },\n};\n</script>\n\n<style scoped>\n.app-container {\n  display: flex;\n  font-family: 'Roboto', sans-serif;\n  min-height: 100vh;\n}\n\n.light {\n  background-color: #f4f7fc;\n  color: #333;\n}\n\n.dark {\n  background-color: #181818;\n  color: #e8e8e8;\n}\n\n.sidebar {\n  width: 300px;\n  background-color: #0070f3;\n  padding: 30px 20px;\n  color: white;\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n}\n\n.app-title {\n  font-size: 26px;\n  font-weight: bold;\n  margin-bottom: 20px;\n}\n\n.nav-links {\n  display: flex;\n  flex-direction: column;\n  width: 100%;\n}\n\n.nav-btn {\n  padding: 12px 20px;\n  margin-bottom: 10px;\n  background-color: transparent;\n  color: white;\n  border: 1px solid white;\n  border-radius: 5px;\n  cursor: pointer;\n  text-align: left;\n  width: 100%;\n}\n\n.nav-btn:hover,\n.nav-btn.active {\n  background-color: rgba(255, 255, 255, 0.1);\n}\n\n.toggle-btn-container {\n  margin-top: auto;\n}\n\n.toggle-btn {\n  padding: 10px 20px;\n  font-size: 14px;\n  background-color: transparent;\n  color: white;\n  border: 1px solid white;\n  border-radius: 5px;\n  cursor: pointer;\n}\n\n.toggle-btn:hover {\n  background-color: rgba(255, 255, 255, 0.1);\n}\n\n.main-content {\n  flex-grow: 1;\n  padding: 40px;\n}\n\n.header {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  margin-bottom: 30px;\n}\n\n.search-container {\n  flex-grow: 1;\n}\n\n.search-input {\n  width: 100%;\n  padding: 10px 15px;\n  font-size: 16px;\n  border: 1px solid #ddd;\n  border-radius: 5px;\n}\n\n.add-bookmark-btn {\n  padding: 12px 20px;\n  font-size: 16px;\n  background-color: #0070f3;\n  color: white;\n  border: none;\n  border-radius: 5px;\n  cursor: pointer;\n}\n\n.add-bookmark-btn:hover {\n  background-color: #0056c1;\n}\n\n.bookmark-list-container {\n  display: flex;\n  flex-wrap: wrap;\n  gap: 20px;\n}\n\n.bookmark-list {\n  display: grid;\n  grid-template-columns: repeat(auto-fill, minmax(250px, 1fr));\n  gap: 20px;\n}\n\n.bookmark-card {\n  background-color: white;\n  border-radius: 8px;\n  padding: 20px;\n  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\n  transition: transform 0.3s;\n}\n\n.bookmark-card:hover {\n  transform: scale(1.05);\n}\n\n.bookmark-card h3 {\n  margin: 0;\n  font-size: 18px;\n}\n\n.bookmark-card p {\n  margin: 10px 0;\n  font-size: 14px;\n}\n\n.bookmark-card button {\n  padding: 8px 16px;\n  background-color: #0070f3;\n  color: white;\n  border: none;\n  border-radius: 5px;\n  cursor: pointer;\n}\n\n.bookmark-card button:hover {\n  background-color: #0056c1;\n}\n\n.app-footer {\n  background-color: #0070f3;\n  color: white;\n  padding: 20px;\n  text-align: center;\n}\n</style>\n"],"mappings":";;;;;AAsDA,SAASA,GAAG,EAAEC,QAAQ,EAAEC,SAAS,EAAEC,KAAI,QAAS,KAAK;AACrD,OAAOC,SAAQ,MAAO,4BAA4B;AAClD,OAAOC,cAAa,MAAO,iCAAiC;AAC5D,OAAOC,YAAW,MAAO,+BAA+B;AAExD,eAAe;EACbC,UAAU,EAAE;IACVH,SAAS;IACTC,cAAc;IACdC;EACF,CAAC;EACDE,KAAKA,CAAA,EAAG;IACN,MAAMC,SAAQ,GAAIT,GAAG,CAAC,EAAE,CAAC;IACzB,MAAMU,cAAa,GAAIV,GAAG,CAAC,EAAE,CAAC;IAC9B,MAAMW,WAAU,GAAIX,GAAG,CAAC,EAAE,CAAC;IAC3B,MAAMY,QAAO,GAAIZ,GAAG,CAAC,KAAK,CAAC;IAC3B,MAAMa,WAAU,GAAIb,GAAG,CAAC,KAAK,CAAC;IAC9B,MAAMc,aAAY,GAAId,GAAG,CAAC,KAAK,CAAC,EAAE;;IAElC;IACA,MAAMe,cAAa,GAAIA,CAAA,KAAM;MAC3BH,QAAQ,CAACI,KAAI,GAAI,CAACJ,QAAQ,CAACI,KAAK;MAChCC,YAAY,CAACC,OAAO,CAAC,UAAU,EAA